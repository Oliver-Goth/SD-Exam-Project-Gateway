{
  "uid": "rabbitmq-overview",
  "title": "RabbitMQ Metrics",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Queued Messages",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages)",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Unacked Messages",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages_unacked)",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Message Publish Rate",
      "gridPos": { "x": 0, "y": 6, "w": 16, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_channel_messages_published_total[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Messages Acknowledged (rate)",
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_channel_messages_acked_total[1m]))",
          "refId": "A"
        }
      ]
    }
  ]
}
