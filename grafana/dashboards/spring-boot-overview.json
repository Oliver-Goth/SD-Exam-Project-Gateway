{
  "uid": "spring-boot-overview",
  "title": "Spring Boot Services Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(http_server_requests_seconds_count, instance)",
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests per Second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{instance=\"$service\"}[1m]))"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Error Rate (4xx / 5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{instance=\"$service\",status=~\"4..|5..\"}[1m]))"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP p95 Latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{instance=\"$service\"}[1m])))"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    }
  ]
}

